{
  "name": "mahjong-client-server",
  "private": true,
  "version": "0.0.0",
  "description": "A websocket-based client/server implementation for mahjong (real mahjong, not 'solitaire')",
  "main": "index.js",
  "scripts": {
    "client:build": "webpack --prod --progress --colors --watch",
    "client:dev": "webpack-dev-server --progress --colors --hot --inline --content-base public/",
    "client": "http-server -s -c-1 apps/client/web",
    "server": "node apps/server/server.js",
    "start": "npm-run-all --parallel client:build client server"
  },
  "author": "Pomax",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.3",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "md5": "^2.1.0",
    "npm-run-all": "^1.7.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "socket.io": "^1.4.5",
    "style-loader": "^0.13.0",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "license": "All rights reserved"
}
