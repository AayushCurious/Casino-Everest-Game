{
  "name": "mahjong-client-server",
  "private": true,
  "version": "0.0.0",
  "description": "A websocket-based client/server implementation for mahjong (real mahjong, not 'solitaire')",
  "main": "index.js",
  "bin": {
    "mahjong-cli": "./bin/mahjong-cli"
  },
  "scripts": {
    "test": "node src/core/test/test.js",
    "cli": "node src/client/cli/test.js",
    "server": "node src/server/server.js",
    "open": "node-open http://localhost:8081/client",
    "dev": "webpack --prod --progress --colors --watch --config ./src/client/web/webpack.config.js",
    "web": "npm-run-all --parallel server dev open"
  },
  "author": "Pomax",
  "dependencies": {
    "arctic-redpoll": "^1.0.0",
    "cli-color": "^1.1.0",
    "inquirer": "^1.0.2",
    "md5": "^2.1.0",
    "pirates": "^2.1.1",
    "socket.io": "^1.4.5",
    "superagent": "^2.0.0-alpha.3",
    "uuid": "^2.0.2",
    "wikijs": "https://github.com/dijs/wiki/archive/parser.tar.gz"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "brain": "^0.7.0",
    "classnames": "^2.2.3",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "habitat": "^3.1.2",
    "less": "^2.6.1",
    "less-watch-compiler": "^1.2.3",
    "npm-run-all": "^1.7.0",
    "open": "0.0.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "style-loader": "^0.13.0",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "license": "All rights reserved"
}
